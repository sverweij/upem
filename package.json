{
  "name": "upem",
  "version": "7.3.1",
  "type": "module",
  "description": "Ups your package.json dependencies to latest. Opinionated. Respectless.",
  "bin": {
    "upem": "dist/cli.js"
  },
  "exports": {
    ".": "./dist/main.js"
  },
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "keywords": [
    "npm",
    "check",
    "updates",
    "dependencies",
    "package.json",
    "version",
    "management",
    "ncu"
  ],
  "author": "Sander Verweij (https://sverweij.github.io/)",
  "license": "MIT",
  "homepage": "https://github.com/sverweij/upem",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sverweij/upem"
  },
  "bugs": {
    "url": "https://github.com/sverweij/upem/issues"
  },
  "devDependencies": {
    "@types/mocha": "10.0.0",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "c8": "7.12.0",
    "dependency-cruiser": "11.18.0",
    "eslint": "8.26.0",
    "eslint-config-moving-meadow": "4.0.2",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-budapestian": "5.0.1",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-mocha": "10.1.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-security": "1.5.0",
    "eslint-plugin-unicorn": "44.0.2",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "mocha": "10.1.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  },
  "dependencies": {
    "libnpmconfig": "^1.2.1"
  },
  "scripts": {
    "build": "npm-run-all build:clean build:ts build:chmod-bin",
    "build:chmod-bin": "chmod 700 dist/cli.js",
    "build:clean": "rm -rf dist",
    "build:ts": "tsc ",
    "build:stage": "git add .",
    "check": "npm-run-all --parallel depcruise lint test",
    "depcruise": "depcruise src --validate --cache",
    "depcruise:graph": "npm-run-all depcruise:graph:html",
    "depcruise:graph:html": "depcruise src --validate --cache --output-type dot | dot -T svg | tee docs/dependencygraph.svg | depcruise-wrap-stream-in-html > docs/dependencygraph.html",
    "depcruise:graph:view": "depcruise src --validate --cache --output-type dot --prefix vscode://file/$(pwd)/ | dot -T svg | depcruise-wrap-stream-in-html | browser",
    "format": "prettier --write \"src/**/*.{ts,js}\" \"*.{json,yml,md}\" .github",
    "format:check": "prettier --check \"src/**/*.{ts,js}\" \"*.{json,yml,md}\" .github",
    "lint": "npm-run-all lint:eslint format:check",
    "lint:eslint": "eslint src types",
    "lint:fix": "npm-run-all lint:fix:eslint format",
    "lint:fix:eslint": "eslint --fix src types",
    "test": "NODE_OPTIONS=--no-warnings c8 --check-coverage --statements 100 --branches 100 --functions 100 --lines 100 --exclude \"{src/**/*.spec.js,src/__mocks__/*,src/__fixtures__/*,.mocharc.cjs}\" --reporter text-summary --reporter html --reporter json-summary mocha",
    "test:watch": "find src | NODE_OPTIONS=--no-warnings entr -c npx mocha",
    "update-dependencies": "npm-run-all upem:update upem:install lint:fix check",
    "upem-outdated": "npm outdated --json --long | dist/cli.js --dry-run",
    "upem:update": "npm outdated --json --long | dist/cli.js | pbcopy && pbpaste",
    "upem:install": "npm install",
    "version": "npm-run-all build check depcruise:graph build:stage",
    "prepare": "husky install"
  },
  "engines": {
    "node": "^14.13.1||>=16"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "depcruise --validate -- src",
      "eslint --fix"
    ],
    "*.{ts,json,md,yml}": [
      "prettier --write"
    ]
  }
}
